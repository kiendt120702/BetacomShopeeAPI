# Shopee API Configuration
# Điền thông tin từ Shopee Open Platform (https://open.shopee.com)

# Partner credentials
VITE_SHOPEE_PARTNER_ID=123456
VITE_SHOPEE_PARTNER_KEY=your_partner_key_here

# Shop ID (optional - sẽ được set sau khi authenticate)
VITE_SHOPEE_SHOP_ID=

# OAuth Callback URL
VITE_SHOPEE_CALLBACK_URL=https://ops.betacom.agency/auth/callback

# Supabase Configuration
# Lấy từ Supabase Dashboard > Settings > API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here
